         _handlePic(){
             setState : selectedPic
            <div className="col-md-6">
                <img src={pictureSkill} alt="logos" /* style="width: 100%;" */ className="mt-5 logo"/>
            </div>
         }

        render(){
            return(
                {selectedPic
                ?(imprime la imagen de la skill)
